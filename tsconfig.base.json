{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {},
    "target": "ES2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "exclude": ["node_modules", "tmp"]
}
